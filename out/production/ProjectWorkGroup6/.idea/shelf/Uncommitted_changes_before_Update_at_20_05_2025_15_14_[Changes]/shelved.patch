+import javafx.scene.canvas.GraphicsContext;
+import org.junit.jupiter.api.BeforeEach;
+import org.junit.jupiter.api.Test;
+import projectworkgroup6.Model.Shape;
+import projectworkgroup6.View.CanvasView;
+import projectworkgroup6.View.ShapeView;
+
+import java.util.Arrays;
+
+import static org.mockito.Mockito.mock;
+import static org.mockito.Mockito.verify;
+
+public class CanvasViewTest {
+
+    private CanvasView canvasView;
+    private GraphicsContext gcMock;
+    private ShapeView shapeViewMock;
+
+    @BeforeEach
+    void setup() {
+        // Mock GraphicsContext con mockito-inline per classi finali
+        gcMock = mock(GraphicsContext.class);
+
+        // Mock ShapeView
+        shapeViewMock = mock(ShapeView.class);
+
+        // Costruisci CanvasView simulando i campi necessari
+        canvasView = new CanvasView(null, null, null, null) {
+            @Override
+            public void render(Iterable<ShapeView> views) {
+                // Override per usare il mock di GraphicsContext
+                // invece del reale
+                for (ShapeView v : views) {
+                    v.draw(gcMock);
+                }
+            }
+        };
+    }
+
+    @Test
+    void testRenderCallsDrawOnShapeViews() {
+        canvasView.render(Arrays.asList(shapeViewMock));
+        verify(shapeViewMock).draw(gcMock);
+    }
+}
